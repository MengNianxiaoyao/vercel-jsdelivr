{
  "rewrites": [
    {
      "source": "/npm/:package@:version/:path*",
      "destination": "https://cdn.jsdelivr.net/npm/:package@:version/:path*",
      "has": [{ "type": "host", "value": "your-domain.com" }]
    },
    {
      "source": "/gh/:user/:repo@:version/:path*",
      "destination": "https://cdn.jsdelivr.net/gh/:user/:repo@:version/:path*",
      "has": [{ "type": "host", "value": "your-domain.com" }]
    }
  ],
  "edgeFunctions": {
    "onRequest": [
      { "src": "/npm/(.*)", "dest": "/whitelist.js" },
      { "src": "/gh/(.*)", "dest": "/whitelist.js" }
    ]
  }
}
